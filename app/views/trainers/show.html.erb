<h3>Trainer</h3>
<p>Name: <%= @trainer.name if @trainer.name %></p>
<p>Email: <%= @trainer.email if @trainer.email %></p>
<% @trainer.pokemons.each do |r| %>
	<%= r.name %>      Level: <%= r.level %>      Health: <%= r.Health %>
	<%= button_to "Damage!", damage_path(id: r.id, trainer_id: @trainer.id), :class => "button medium", :method => :patch %>
	<br />
<% end %>
<%= button_to "Creat New Pokemon", pageToNewPokemon_path, :class => "button medium", :method => :get %>
